<?xml version='1.0' encoding='UTF-8'?>
<odoo>
  <record id="studio_customization.odoo_studio_res_part_9f9f5f00-ba1b-4d53-823f-6a20203ee462" model="ir.ui.view" context="{'studio': True}">
    <field name="active" eval="True"/>
    <field name="arch" type="xml">
      <data>
        <xpath expr="//field[@name='display_name']" position="after">
          <field name="company_registry" optional="show" string="CUSTNO"/>
        </xpath>
      </data>
    </field>
    <field name="field_parent" eval="False"/>
    <field name="groups_id" eval="[(6, 0, [])]"/>
    <field name="inherit_id" ref="base.view_partner_tree"/>
    <field name="key" eval="False"/>
    <field name="mode">extension</field>
    <field name="model">res.partner</field>
    <field name="name">Odoo Studio: res.partner.tree customization</field>
    <field name="priority">99</field>
    <field name="type">tree</field>
  </record>
  <record id="studio_customization.odoo_studio_res_part_a7f5d362-44c6-4ebe-9a32-3ee547104133" model="ir.ui.view" context="{'studio': True}">
    <field name="active" eval="True"/>
    <field name="arch" type="xml">
      <data>
        <xpath expr="//kanban[1]/templates[1]/t[1]/div[1]/div[2]/div[1]/div[1]/span[1]/field[@name='category_id']" position="after">
          <field name="company_registry" display="full"/>
        </xpath>
      </data>
    </field>
    <field name="field_parent" eval="False"/>
    <field name="groups_id" eval="[(6, 0, [])]"/>
    <field name="inherit_id" ref="base.res_partner_kanban_view"/>
    <field name="key" eval="False"/>
    <field name="mode">extension</field>
    <field name="model">res.partner</field>
    <field name="name">Odoo Studio: res.partner.kanban customization</field>
    <field name="priority">99</field>
    <field name="type">kanban</field>
  </record>
</odoo>
